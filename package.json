{
  "name": "askql",
  "version": "1.1.0",
  "description": "AskQL is a query language that can express every data request",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --config jest.test.config.js",
    "test:dev": "jest --watch --config jest.test.config.js",
    "ask": "ASK_PRINT_RESULT=1 jest --config jest.test.config.js --no-cache",
    "check": "tsc --noEmit",
    "tsc": "tsc",
    "tsc:test": "tsc && npm run test --no-cache",
    "build:test": "npm run build && npm run test --no-cache",
    "prebuild": "ts-node scripts/preBuild.ts && tsc",
    "build": "jest --config jest.build.config.js --no-cache && npm run playground:build",
    "postbuild": "ts-node scripts/postBuild.ts",
    "prettier": "prettier --write --config prettier.config.js .",
    "prettier:check": "prettier --config prettier.config.js --check '**/*.{js,jsx,ts,tsx}'",
    "prettier:scripts": "prettier --write ./scripts",
    "lint:js": "eslint . --ext .js,.tsx,.ts",
    "lint:js:fix": "npm run lint:js --fix",
    "playground:build": "rm -rf dist/playground && mkdir -p dist/playground/public && npm run playground:build-assets && npm run playground:build-ts && npm run playground:copy-static-assets",
    "playground:build-assets": "npm run playground:build-assets:img",
    "playground:build-assets:img": "imagemin src/playground/public/assets/img/* --out-dir=dist/playground/public/img",
    "playground:build-ts": "tsc && mv dist/playground/public/assets/js dist/playground/public/js",
    "playground:copy-static-assets": "ts-node src/playground/copyStaticAssets.ts",
    "playground:start": "node dist/playground/app.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "bin": {
    "askql": "dist/cli.js",
    "askqlServer": "dist/demoServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xFAANG/askql.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xFAANG/askql/issues"
  },
  "homepage": "https://askql.org/",
  "engines": {
    "node": ">=12.14.0"
  },
  "dependencies": {},
  "devDependencies": {
    "handlebars": "^4.7.6",
    "@babel/polyfill": "^7.10.1",
    "@types/ace": "0.0.43",
    "babel-loader": "^8.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "golden-layout": "^1.5.9",
    "imagemin-cli": "^6.0.0",
    "node-sass": "^4.14.1",
    "npm-watch": "^0.6.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "@types/cors": "^2.8.6",
    "@types/express": "4.17.6",
    "@types/express-handlebars": "^3.1.0",
    "@types/glob": "7.1.2",
    "@types/jest": "26.0.0",
    "@types/jsesc": "2.5.1",
    "@types/lodash": "^4.14.157",
    "@types/micromatch": "^4.0.1",
    "@types/node": "14.0.13",
    "@types/node-fetch": "2.5.7",
    "@types/pegjs": "0.10.2",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "babel-jest": "26.0.1",
    "babel-plugin-inline-import": "3.0.0",
    "chalk": "4.1.0",
    "dotenv": "^8.2.0",
    "cors": "^2.8.5",
    "eslint": "7.3.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "express": "4.17.1",
    "express-handlebars": "^4.0.4",
    "fs.promises": "0.1.2",
    "glob": "7.1.6",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jsesc": "3.0.1",
    "lint-staged": "10.2.11",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "nanoid": "^3.1.10",
    "node-fetch": "2.6.0",
    "pegjs": "0.10.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
