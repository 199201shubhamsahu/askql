ask {
  const covid = [{country: 'USA', newInfected: 69128, newDeaths: 1047, newRecovered: 19342}, {country: 'Spain', newInfected: 13598, newDeaths: 217, newRecovered: 578}, {country: 'Russia', newInfected: 1551, newDeaths: 93, newRecovered: 178}, {country: 'Italy', newInfected: 992, newDeaths: 262, newRecovered: 688}, {country: 'UK', newInfected: 3687, newDeaths: 428, newRecovered: 19342}]
  log('map: ', map(get('covid'), fun(c: any): any {
    return at(get('c'), 'newInfected')
  }))
  const totalNewInfected = sum(map(get('covid'), fun(c: any): any {
    return at(get('c'), 'newInfected')
  }))
  const totalNewDeaths = sum(map(get('covid'), fun(a: any): any {
    return at(get('a'), 'newDeaths')
  }))
  const totalnewRecovered = sum(map(get('covid'), fun(a: any): any {
    return at(get('a'), 'newRecovered')
  }))
  const total = sum([get('totalNewInfected'), get('totalNewDeaths'), get('totalnewRecovered')])
  log('Total new infected - ', get('totalNewInfected'))
  log('Total new deaths - ', get('totalNewDeaths'))
  log('Total new recovered - ', get('totalnewRecovered'))
  log('Total - ', get('total'))
  return {totalNewDeaths: get('totalNewDeaths'), totalNewInfected: get('totalNewInfected'), totalnewRecovered: get('totalnewRecovered'), total: get('total')}
}