ask(arg1: int): string {
  const factorial = fun(n: int): int {
    if (lessThan(get('n'), 2)) {
      return get('n')
    }
    multiply(get('n'), factorial(minus(get('n'), 1)))
  }
  const sum = fun(a: int, b: int): int {
    plus(get('a'), get('b'))
  }
  const one = 1
  toString(sum(get('one'), factorial(get('arg1'))))
}